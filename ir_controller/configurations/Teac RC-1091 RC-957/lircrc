### Playback commands ###
begin
prog = irexec
button = KEY_POWER
config = /usr/local/bin/volumio stop
end
begin
prog = irexec
button = KEY_PLAY
config = /usr/local/bin/volumio play
end
begin
prog = irexec
button = KEY_PAUSE
config = /usr/local/bin/volumio pause
end
begin
prog = irexec
button = KEY_STOP
config = /usr/local/bin/volumio stop
end
begin
prog = irexec
button = KEY_PREVIOUS
config = /usr/local/bin/volumio previous
end
begin
prog = irexec
button = KEY_NEXT
config = /usr/local/bin/volumio next
end
begin
prog = irexec
button = KEY_FASTFORWARD
config = /usr/local/bin/volumio seek plus
end
begin
prog = irexec
button = KEY_REWIND
config = /usr/local/bin/volumio seek minus
end
### Volume commands ###
begin
prog = irexec
button = KEY_VOLUMEUP
config = /usr/local/bin/volumio volume plus
end
begin
prog = irexec
button = KEY_VOLUMEDOWN
config = /usr/local/bin/volumio volume minus
end
begin
prog = irexec
button = tuner_up
config = curl -X POST -H "Content-Type: application/json" -d '{"control":"vol-up"}' http://127.0.0.1:8889/post_listener
end
begin
prog = irexec
button = tuner_down
config = curl -X POST -H "Content-Type: application/json" -d '{"control":"vol-down"}' http://127.0.0.1:8889/post_listener
end
begin
prog = irexec
button = tuner_enter
config = curl -X POST -H "Content-Type: application/json" -d '{"control":"enter"}' http://127.0.0.1:8889/post_listener
end
begin
prog = irexec
button = tuner_function
config = curl -X POST -H "Content-Type: application/json" -d '{"control":"functon-fm-mode"}' http://127.0.0.1:8889/post_listener
end
begin
prog = irexec
button = tuner_info-rds
config = curl -X POST -H "Content-Type: application/json" -d '{"control":"info"}' http://127.0.0.1:8889/post_listener
end
begin
prog = irexec
button = tuner_band
config = curl -X POST -H "Content-Type: application/json" -d '{"control":"band"}' http://127.0.0.1:8889/post_listener
end
begin
prog = irexec
button = program
config = curl -X POST -H "Content-Type: application/json" -d '{"control":"memory"}' http://127.0.0.1:8889/post_listener
end
begin
prog = irexec
button = KEY_POWER
config = /sbin/poweroff -p
end